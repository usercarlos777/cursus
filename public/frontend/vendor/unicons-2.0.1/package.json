    "@babel/preset-env": "^7.14.8",
    "babel-loader": "^8.2.2",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "regenerator-runtime": "^0.13.9",
    "webpack": "^5.48.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "async": "^2.6.3",
    "axios": "^0.21.1",
